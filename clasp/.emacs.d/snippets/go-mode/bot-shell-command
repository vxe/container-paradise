# -*- mode: snippet; require-final-newline: nil -*-
# name: bot-shell-command
# key: botshellcommand
# binding: direct-keybinding
# --
func (api *App) Run(msg parotplugins.Message) (output interface{}, err error) {
	//	output = msg.From.Full()
	out, err := exec.Command("${1:shell-command}").Output()
	if err != nil {
		log.Fatal(err)
	}
	output = string(out)
	return
}