export SITE= ;clush -s $SITE -g api -g apift -g cimage -g csearch -g cshared -g cstring -g cstate -g crelay -g lyraa -g lyrab -B 'sudo ethtool -i \`sudo facter -p default_interface\` | grep driver  | awk -F: "{print \$2}"; modinfo ixgbe | grep version | awk -F: "{print \$2}" | head -1' | sort