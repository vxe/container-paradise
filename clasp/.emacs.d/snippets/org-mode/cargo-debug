# -*- mode: snippet -*-
# name: cargo-debug
# key: <cargo
# --
 pdsh -w "$1.$2" 'sudo ethtool -i \`sudo facter -p default_interface\` | grep driver  | awk -F: "{print \$2}"; modinfo ixgbe | grep version | awk -F: "{print \$2}" | head -1' | sort
 pdsh -w "$1.$2" "sudo ethtool \\$(sudo facter -p default_interface) | grep -i speed"
 pdsh -w "$1.$2" "dmesg -T | grep -i error | wc -l"
 pdsh -w "$1.$2" "lsscsi | wc -l"
${4:#} pdsh -w '$1.$2' '/opt/parsec/bin/cargo-ab.py -c 5 -n 50 -s httpsss+cargo://cargo.$2.parsec.apple.com/HTTP_SSS_$3 -v  | tee ~/var/log/cargo-ab.log | grep "100%"'
${5:#} pdsh -w "$1.$2" 'cat ~/var/log/cargo-ab.log'  2>/dev/null