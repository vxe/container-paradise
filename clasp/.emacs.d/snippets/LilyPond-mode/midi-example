# -*- mode: snippet -*-
# name: midi-example
# key: <midi
# --
#(ly:set-option 'midi-extension "midi")

\score {
\new DrumStaff <<
  \time $1/$2
  \drummode {
    bd4 sn4 bd4 sn4
    %begin polyphony
    << {
      \repeat unfold 16 hh16
    } \\\ {
      bd4 sn4 bd4 sn4
    } >>
  }
>>

\layout { }
\midi { }
}